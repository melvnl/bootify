    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "postcss": "^8.4.13",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.24",
    "typescript": "4.5.5"
